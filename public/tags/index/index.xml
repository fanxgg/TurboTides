    <item>
      <title>Case Studies</title>
      <link>https://www.turbotides.com/blog/</link>
      <pubDate>Sat, 18 Jan 2025 00:00:00 +0000</pubDate>
      <guid>https://www.turbotides.com/blog/</guid>
      <description>&lt;style&gt;&#xD;&#xA;    .case-timeline {&#xD;&#xA;        position: relative;&#xD;&#xA;        max-width: 1200px;&#xD;&#xA;        margin: 0 auto;&#xD;&#xA;        padding: 40px 0;&#xD;&#xA;        min-height: 680px;&#xD;&#xA;        display: flex;&#xD;&#xA;        flex-direction: column;&#xD;&#xA;        justify-content: space-between;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    .case-timeline::before {&#xD;&#xA;        content: &#39;&#39;;&#xD;&#xA;        position: absolute;&#xD;&#xA;        height: 100%;&#xD;&#xA;        width: 4px;&#xD;&#xA;        background: linear-gradient(180deg, #3b82f6 0%, #4472c4 100%);&#xD;&#xA;        left: 50%;&#xD;&#xA;        transform: translateX(-50%);&#xD;&#xA;        z-index: 0;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;     &#xD;&#xA;    .timeline-node:nth-child(1) {&#xD;&#xA;        top: 5%;  &#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    .timeline-node:nth-child(2) {&#xD;&#xA;        top: 30%;  &#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    .timeline-node:nth-child(3) {&#xD;&#xA;        top: 55%;  &#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    .timeline-node:nth-child(4) {&#xD;&#xA;        top: 80%;  &#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    .timeline-node {&#xD;&#xA;        position: relative;&#xD;&#xA;        margin: 0;&#xD;&#xA;        height: 140px;&#xD;&#xA;        display: flex;&#xD;&#xA;        justify-content: center;&#xD;&#xA;        align-items: center;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    .timeline-node:nth-child(odd) .timeline-content {&#xD;&#xA;        left: 55%;&#xD;&#xA;        top: 0;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    .timeline-node:nth-child(even) .timeline-content {&#xD;&#xA;        right: 55%;&#xD;&#xA;        top: 0;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    .timeline-content {&#xD;&#xA;        background: white;&#xD;&#xA;        border-radius: 10px;&#xD;&#xA;        box-shadow: 0 4px 16px rgba(0,0,0,0.1);&#xD;&#xA;        padding: 25px;&#xD;&#xA;        width: 40%;&#xD;&#xA;        position: absolute;&#xD;&#xA;        transition: transform 0.3s, box-shadow 0.3s;&#xD;&#xA;        z-index: 1;&#xD;&#xA;        min-height: 140px;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    .timeline-content:hover {&#xD;&#xA;        transform: translateY(-5px);&#xD;&#xA;        box-shadow: 0 10px 25px rgba(0,0,0,0.15);&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    .timeline-dot {&#xD;&#xA;        width: 28px;&#xD;&#xA;        height: 28px;&#xD;&#xA;        border-radius: 50%;&#xD;&#xA;        background: #4472c4;&#xD;&#xA;        border: 4px solid white;&#xD;&#xA;        box-shadow: 0 2px 5px rgba(0,0,0,0.2);&#xD;&#xA;        position: absolute;&#xD;&#xA;        top: 50%;&#xD;&#xA;        left: 50%;&#xD;&#xA;        transform: translate(-50%, -50%);&#xD;&#xA;        z-index: 2;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    .timeline-title {&#xD;&#xA;        color: #333;&#xD;&#xA;        font-size: 1.5rem;&#xD;&#xA;        font-weight: bold;&#xD;&#xA;        margin-bottom: 10px;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    .timeline-count {&#xD;&#xA;        color: #777;&#xD;&#xA;        font-size: 1rem;&#xD;&#xA;        margin-bottom: 15px;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    .timeline-btn {&#xD;&#xA;        display: inline-block;&#xD;&#xA;        padding: 8px 16px;&#xD;&#xA;        background-color: #4472c4;&#xD;&#xA;        color: white;&#xD;&#xA;        border-radius: 5px;&#xD;&#xA;        text-decoration: none;&#xD;&#xA;        font-weight: 500;&#xD;&#xA;        transition: background-color 0.3s;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    .timeline-btn:hover {&#xD;&#xA;        background-color: #365899;&#xD;&#xA;    }&#xD;&#xA;    &#xD;&#xA;    @media (max-width: 768px) {&#xD;&#xA;        .case-timeline {&#xD;&#xA;            min-height: 780px;  &#xD;&#xA;            padding-left: 30px;  &#xD;&#xA;        }&#xD;&#xA;        .case-timeline::before {&#xD;&#xA;            left: 30px;&#xD;&#xA;        }&#xD;&#xA;        &#xD;&#xA;        .timeline-node {&#xD;&#xA;            justify-content: flex-start;&#xD;&#xA;            margin: 0;&#xD;&#xA;            min-height: 150px;&#xD;&#xA;        }&#xD;&#xA;&#xD;&#xA;         &#xD;&#xA;        .timeline-node:nth-child(1) { top: 5%; }&#xD;&#xA;        .timeline-node:nth-child(2) { top: 28%; }&#xD;&#xA;        .timeline-node:nth-child(3) { top: 51%; }&#xD;&#xA;        .timeline-node:nth-child(4) { top: 74%; }&#xD;&#xA;        &#xD;&#xA;        .timeline-node:nth-child(odd) .timeline-content,&#xD;&#xA;        .timeline-node:nth-child(even) .timeline-content {&#xD;&#xA;            left: 50px;&#xD;&#xA;            right: auto;&#xD;&#xA;            width: calc(100% - 80px);&#xD;&#xA;            position: relative;  &#xD;&#xA;            top: 0;&#xD;&#xA;        }&#xD;&#xA;        &#xD;&#xA;        .timeline-dot {&#xD;&#xA;            left: 0;&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;    &lt;/style&gt;&#xD;&#xA;    &#xD;&#xA;    &lt;div class=&#34;case-timeline&#34;&gt;&#xD;&#xA;        &lt;div class=&#34;timeline-node&#34;&gt;&#xD;&#xA;            &lt;div class=&#34;timeline-dot&#34;&gt;&lt;/div&gt;&#xD;&#xA;            &lt;div class=&#34;timeline-content&#34;&gt;&#xD;&#xA;                &lt;h3 class=&#34;timeline-title&#34;&gt;TurboTides&lt;/h3&gt;&#xD;&#xA;                &#xD;&#xA;                    &#xD;&#xA;                &lt;p class=&#34;timeline-count&#34;&gt;&#xD;&#xA;                    20 &#xD;&#xA;                    cases&#xD;&#xA;                &lt;/p&gt;</description>
    </item>
